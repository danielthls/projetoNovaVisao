program ProjectNotification;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  Notify;

begin

  Ntfy.Subscribe('notify-delphi-integration-8jh27d',
    procedure (AEvent: INotifyEvent)
    begin
      Writeln('Você recebeu uma notificação:');
      Writeln('Titulo: ' + AEvent.Title);
      Writeln('Mensagem: ' + AEvent.MessageContent);
    end);

end.
